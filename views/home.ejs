<%- include("partials/header.ejs") %>

<div class="content px-5 my-5" style="width: 50%;">
    <% posts.forEach(post => { %>
        <h1><a href="/views/<%= posts.indexOf(post) %>"><%= post.title %></a></h1>
        <p class="lead mb-4"><%= post.content %></p>
    <% }) %>
</div>

